CREATE TABLE IF NOT EXISTS viaops_outage_manager (
    id INT AUTO_INCREMENT,
    user_id VARCHAR(25) NOT NULL,
    start_epoch BIGINT(20) NOT NULL,
    end_epoch BIGINT(20) NOT NULL,
    subject_line VARCHAR(255) NOT NULL,
    description VARCHAR(1024),
    reason VARCHAR(255),
    active TINYINT(1),
    PRIMARY KEY (id)
)  